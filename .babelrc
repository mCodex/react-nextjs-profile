{
  "presets": [
    "next/babel"
  ],
  "plugins": [
    "transform-runtime",
    "transform-react-inline-elements",
    ["styled-components", { "ssr": true, "displayName": true, "preprocess": false } ]
  ],
  "env": {
    "development": {
      "presets": ["next/babel"]
    },
    "production": {
      "presets": ["next/babel"]
    },
    "test": {
      "presets": [["next/babel", { "preset-env": { "modules": "commonjs" } }]]
    }
  }
}
